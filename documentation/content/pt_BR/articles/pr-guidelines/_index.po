#: documentation/content/en/articles/pr-guidelines/_index.adoc:106
msgid "<<pr-unassigned>>"
msgstr ""

#. type: Plain text
#: documentation/content/en/articles/pr-guidelines/_index.adoc:107
msgid "<<pr-assigned>>"
msgstr ""

