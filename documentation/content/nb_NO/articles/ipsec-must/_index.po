#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:165
#, no-wrap
msgid ""
"  Ref: J. Cryptology v 5 no 2, 1992 pp 89-105\n"
"  also on the web somewhere, which is where I found it.\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:168
#, no-wrap
msgid ""
"  -David Honig\n"
"  honig@sprynet.com\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:173
#, no-wrap
msgid ""
"  Usage:\n"
"  ULISCAN filename\n"
"  outputs to stdout\n"
"*/\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:179
#, no-wrap
msgid ""
"#define L 8\n"
"#define V (1<<L)\n"
"#define Q (10*V)\n"
"#define K (100   *Q)\n"
"#define MAXSAMP (Q + K)\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:182
#, no-wrap
msgid ""
"#include <stdio.h>\n"
"#include <math.h>\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:194
#, no-wrap
msgid ""
"int main(argc, argv)\n"
"int argc;\n"
"char **argv;\n"
"{\n"
"  FILE *fptr;\n"
"  int i,j;\n"
"  int b, c;\n"
"  int table[V];\n"
"  double sum = 0.0;\n"
"  int iproduct = 1;\n"
"  int run;\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:196
#, no-wrap
msgid "  extern double   log(/* double x */);\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:198
#, no-wrap
msgid "  printf(\"Uliscan 21 Dec 98 \\nL=%d %d %d \\n\", L, V, MAXSAMP);\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:205
#, no-wrap
msgid ""
"  if (argc < 2) {\n"
"    printf(\"Usage: Uliscan filename\\n\");\n"
"    exit(-1);\n"
"  } else {\n"
"    printf(\"Measuring file %s\\n\", argv[1]);\n"
"  }\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:207
#, no-wrap
msgid "  fptr = fopen(argv[1],\"rb\");\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:212
#, no-wrap
msgid ""
"  if (fptr == NULL) {\n"
"    printf(\"Can't find %s\\n\", argv[1]);\n"
"    exit(-1);\n"
"  }\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:216
#, no-wrap
msgid ""
"  for (i = 0; i < V; i++) {\n"
"    table[i] = 0;\n"
"  }\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:221
#, no-wrap
msgid ""
"  for (i = 0; i < Q; i++) {\n"
"    b = fgetc(fptr);\n"
"    table[b] = i;\n"
"  }\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:223
#, no-wrap
msgid "  printf(\"Init done\\n\");\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:225
#, no-wrap
msgid "  printf(\"Expected value for L=8 is 7.1836656\\n\");\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:227
#, no-wrap
msgid "  run = 1;\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:231
#, no-wrap
msgid ""
"  while (run) {\n"
"    sum = 0.0;\n"
"    iproduct = 1;\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:236
#, no-wrap
msgid ""
"    if (run)\n"
"      for (i = Q; run && i < Q + K; i++) {\n"
"        j = i;\n"
"        b = fgetc(fptr);\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:239
#, no-wrap
msgid ""
"        if (b < 0)\n"
"          run = 0;\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:243
#, no-wrap
msgid ""
"        if (run) {\n"
"          if (table[b] > j)\n"
"            j += K;\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:245
#, no-wrap
msgid "          sum += log((double)(j-table[b]));\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:249
#, no-wrap
msgid ""
"          table[b] = i;\n"
"        }\n"
"      }\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:252
#, no-wrap
msgid ""
"    if (!run)\n"
"      printf(\"Premature end of file; read %d blocks.\\n\", i - Q);\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:255
#, no-wrap
msgid ""
"    sum = (sum/((double)(i - Q))) /  log(2.0);\n"
"    printf(\"%4.4f \", sum);\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:258
#, no-wrap
msgid ""
"    for (i = 0; i < (int)(sum*8.0 + 0.50); i++)\n"
"      printf(\"-\");\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:260
#, no-wrap
msgid "    printf(\"\\n\");\n"
msgstr ""

#. type: delimited block . 4
#: documentation/content/en/articles/ipsec-must/_index.adoc:274
#, no-wrap
msgid ""
"    /* refill initial table */\n"
"    if (0) {\n"
"      for (i = 0; i < Q; i++) {\n"
"        b = fgetc(fptr);\n"
"        if (b < 0) {\n"
"          run = 0;\n"
"        } else {\n"
"          table[b] = i;\n"
"        }\n"
"      }\n"
"    }\n"
"  }\n"
"}\n"
msgstr ""
